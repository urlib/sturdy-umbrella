	//var scrollPoint = $(".title:first").offset().top-$(".headerWrapper:first").height();
	var scrollPoint = $(".yearBar:first").offset().top-$(".headerWrapper:first").height();
	//console.log(scrollPoint);
	$(document).scrollTop(scrollPoint);
//});
